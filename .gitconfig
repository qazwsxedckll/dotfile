[pull]
	rebase = true
[user]
	email = decai.wang@foxmail.com
	name = evilolipop
[core]
	editor = vim
[init]
	defaultBranch = main
[fetch]
	prune = true
[push]
	followTags = true
[alias]
	prune-gone = "!git fetch -p && for branch in $(git for-each-ref --format '%(refname) %(upstream:track)' refs/heads | awk '$2 == \"[gone]\" {sub(\"refs/heads/\", \"\", $1); print $1}'); do git branch -D $branch; done"